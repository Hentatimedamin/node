<html>
    <%-include('./partials/head.ejs')%>
    <body>
       <%-include('./partials/nav.ejs')%>
       <div class="content">
        <h2>
            All blogs
        </h2>      
        <% if (blogs.length>0){ %>
            <% blogs.forEach(blog=>{%>
               <a href="/blogs/<%=blog._id%>">
                <h3 class="title">
                    <%=blog.title%>
                </h3>
               </a>
                <p class="snippet">
                    <%=blog.snippet%>
                </p>
                <p class="snippet">
                    <%=blog.body%>
                </p>
            <%})%>
        <%}%>
       </div>
    </body>
</html>